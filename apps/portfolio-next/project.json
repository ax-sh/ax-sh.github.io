{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/portfolio-next",
	"projectType": "application",
	"targets": {
		"deploy": {
			"executor": "@nrwl/workspace:run-commands",
			"configurations": {
				"netlify": { "commands": ["netlify deploy --build --prod"] }
			}
		},
		"build": {
			"executor": "@nrwl/next:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"root": "apps/portfolio-next",
				"outputPath": "dist/apps/portfolio-next"
			},
			"configurations": {
				"development": {
					"outputPath": "apps/portfolio-next"
				},
				"production": {}
			}
		},
		"serve": {
			"executor": "@nrwl/next:server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "portfolio-next:build",
				"dev": true
			},
			"configurations": {
				"development": {
					"buildTarget": "portfolio-next:build:development",
					"dev": true
				},
				"production": {
					"buildTarget": "portfolio-next:build:production",
					"dev": false
				}
			}
		},
		"export": {
			"executor": "@nrwl/next:export",
			"options": {
				"buildTarget": "portfolio-next:build:production"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/portfolio-next/**/*.{ts,tsx,js,jsx}"]
			}
		}
	},
	"tags": []
}
